
public class MaxMin {
	
	public static void main(String[] args) {
		int[] intArray= {2,1,3,1,10,100,5,2};
		
		int min=intArray[0]; //2
		for(int i=0;i<intArray.length;i++)
		{
			if(intArray[i]<min)// 2<2-No  1<2-yes
			{
				
				min=intArray[i];
				System.out.println(min);
			}
			
			/*else if(intArray[i]>min)
			{
				min=intArray[i];
				System.out.println(min);
			}*/
			
			
		}
	}

}
