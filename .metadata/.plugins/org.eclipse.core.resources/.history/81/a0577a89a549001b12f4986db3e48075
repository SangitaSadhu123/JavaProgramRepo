
public class NonZeroPosition {

	public static void main(String[] args) {
		int[] arr={1,0,5,0,2,3,0,8};
		int temp;
		//c=0;
		
		for(int i=0;i<=arr.length;i++)
		{
			while(arr[i]!=0)
			{
			temp=arr[i];
			System.out.println(temp);
			arr[i]=arr[i+1];
			arr[i+1]=temp;
			//c=c+1;
			}
		}
		for(int i=0;i<arr.length;i++)
		{
			System.out.println(arr[i]);
		}
		
		
		

	}

}
