
public class PrimeNumber {

	public static void main(String[] args) {
		int[] a= {1,2,3,4,5};
		int fg=0;
		
		
		for(int i=0; i<a.length; i++){
			fg=0;
	        
			for(int j=2;j<=(int)Math.sqrt(i);j++)
			{
				if(a[i]%j==0)
				{
					fg=1;
					break;
					
				}
			}
				if(fg==0)
					System.out.println(a[i]);	
			
		}
	}
}


