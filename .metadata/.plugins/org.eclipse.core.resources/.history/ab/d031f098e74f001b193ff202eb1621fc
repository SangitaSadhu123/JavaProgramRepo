
public class MaxMin {
	
	public static void main(String[] args) {
		int[] arr= {2,1,3,1,10,100,5,2};
		
		int min=arr[0]; //2
		for(int i=0;i<arr.length;i++)
		{
			if(arr[i]<min)// 2<2-No  1<2-yes
			{
				
				min=arr[i];
				System.out.println(min);
			}
			
			if(arr[i]>min)// 2<2-No  1<2-yes
			{
				
				min=arr[i];
				System.out.println("maximum "+min);
			}
			
			/*else if(intArray[i]>min)
			{
				min=intArray[i];
				System.out.println(min);
			}*/
			
			
		}
	}

}
