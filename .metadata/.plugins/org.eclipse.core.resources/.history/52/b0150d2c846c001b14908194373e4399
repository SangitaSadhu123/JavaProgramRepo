 class Father 
{
public static void swimmer()
	{
		System.out.println("good swimmer");
	}

}

	
public class DemoInheritance
{
	public DemoInheritance()
	{
		
		System.out.println("DemoInheritance is executing ");
	}
	public static void main(String[] args) {
	
		Father.swimmer();
		Son.swimmer();
		
		GrandSon.swimmer();
		GrandSon.Painter();
		GrandSon.singer();
		
		
	}

}
